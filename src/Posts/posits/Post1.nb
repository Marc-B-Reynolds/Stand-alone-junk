Cell[BoxData[
 RowBox[{
  RowBox[{"heron", "[", 
   RowBox[{"a_", ",", "b_", ",", "c_"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "s", ",", "r", ",", "s0", ",", "t0", ",", "r0", ",", "s1", ",", "t1", 
      ",", "r1"}], "}"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{"compute", " ", "exact"}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"s", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"a", "+", "b", "+", "c"}], ")"}], "/", "2"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"r", "=", 
      RowBox[{"Sqrt", "[", 
       RowBox[{"s", 
        RowBox[{"(", 
         RowBox[{"s", "-", "a"}], ")"}], 
        RowBox[{"(", 
         RowBox[{"s", "-", "b"}], ")"}], 
        RowBox[{"(", 
         RowBox[{"s", "-", "c"}], ")"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"posit", " ", "values"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"s0", "=", 
      OverscriptBox[
       RowBox[{
        RowBox[{"(", 
         OverscriptBox[
          RowBox[{
           RowBox[{"(", 
            OverscriptBox[
             RowBox[{"a", "+", "b"}], "_"], ")"}], "+", "c"}], "_"], ")"}], 
        "/", "2"}], "_"]}], ";", "\[IndentingNewLine]", 
     RowBox[{"t0", "=", 
      OverscriptBox[
       RowBox[{
        RowBox[{"(", 
         OverscriptBox[
          RowBox[{
           RowBox[{"(", 
            OverscriptBox[
             RowBox[{"s0", "\[Times]", 
              RowBox[{"(", 
               OverscriptBox[
                RowBox[{"s0", "-", "a"}], "_"], ")"}]}], "_"], ")"}], 
           "\[Times]", 
           RowBox[{"(", 
            OverscriptBox[
             RowBox[{"s0", "-", "b"}], "_"], ")"}]}], "_"], ")"}], "\[Times]", 
        RowBox[{"(", 
         OverscriptBox[
          RowBox[{"s0", "-", "c"}], "_"], ")"}]}], "_"]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"r0", "=", 
      OverscriptBox[
       SqrtBox["t0"], "_"]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"IEEE", " ", "values"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"s1", "=", 
      UnderscriptBox[
       RowBox[{
        RowBox[{"(", 
         UnderscriptBox[
          RowBox[{
           RowBox[{"(", 
            UnderscriptBox[
             RowBox[{"a", "+", "b"}], "_"], ")"}], "+", "c"}], "_"], ")"}], 
        "/", "2"}], "_"]}], ";", "\[IndentingNewLine]", 
     RowBox[{"t1", "=", 
      UnderscriptBox[
       RowBox[{
        UnderscriptBox[
         RowBox[{"(", 
          RowBox[{
           RowBox[{"(", 
            UnderscriptBox[
             RowBox[{"s1", "\[Times]", 
              RowBox[{"(", 
               UnderscriptBox[
                RowBox[{"s1", "-", "a"}], "_"], ")"}]}], "_"], ")"}], 
           "\[Times]", 
           RowBox[{"(", 
            UnderscriptBox[
             RowBox[{"s1", "-", "b"}], "_"], ")"}]}], ")"}], "_"], "\[Times]", 
        RowBox[{"(", 
         UnderscriptBox[
          RowBox[{"s1", "-", "c"}], "_"], ")"}]}], "_"]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"r1", "=", 
      UnderscriptBox[
       SqrtBox["t1"], "_"]}], ";", "\[IndentingNewLine]", 
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"r", ",", " ", 
         RowBox[{"ScientificForm", "[", 
          RowBox[{"N", "[", 
           RowBox[{"r", ",", "exactDecDigits"}], "]"}], "]"}], ",", " ", 
         RowBox[{"BaseForm", "[", 
          RowBox[{
           RowBox[{"N", "[", 
            RowBox[{"r", ",", "exactDecDigits"}], "]"}], ",", "2"}], "]"}]}], 
        "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"r0", ",", 
         RowBox[{"ScientificForm", "[", 
          RowBox[{"N", "[", 
           RowBox[{"r0", ",", "modelDecDigits"}], "]"}], "]"}], ",", " ", 
         RowBox[{"ftobin", "[", "r0", "]"}]}], "}"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"r1", ",", 
         RowBox[{"ScientificForm", "[", 
          RowBox[{"N", "[", 
           RowBox[{"r1", ",", "modelDecDigits"}], "]"}], "]"}], ",", " ", 
         RowBox[{"ftobin", "[", "r1", "]"}]}], "}"}]}], "\[IndentingNewLine]",
       "}"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
