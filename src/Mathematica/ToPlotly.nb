         RowBox[{"y", "=", 
          RowBox[{
           RowBox[{"t", "[", 
            RowBox[{"[", "1", "]"}], "]"}], "[", 
           RowBox[{"[", 
            RowBox[{"All", ",", "2"}], "]"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"y", "=", 
          RowBox[{"\"\<\\ny\>\"", "<>", 
           RowBox[{"toPlotlyCoord", "[", "y", "]"}]}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"js", " ", "=", " ", 
          RowBox[{
          "js", " ", "<>", " ", "x", " ", "<>", " ", "y", " ", "<>", " ", 
           "\"\<\\nmode: 'lines'\\n};\\n\\n\>\""}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"lines", " ", "=", " ", 
          RowBox[{"Drop", "[", 
           RowBox[{"lines", ",", "1"}], "]"}]}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", "js"}]}], 
