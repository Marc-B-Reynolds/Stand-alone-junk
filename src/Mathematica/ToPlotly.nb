   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"StringLength", "[", "e", "]"}], "\[Equal]", "0"}], ",", " ", 
     "m", ",", 
     RowBox[{"m", "<>", "\"\<e\>\"", "<>", "e"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.6729971785702915`*^9, 3.6729971844432936`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"toPlotlyCoord", "[", "x_", "]"}], ":=", 
   RowBox[{"StringReplace", "[", 
